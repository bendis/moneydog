<h1><%=t'New fixed_income'%></h1>

<% form_for(@fixed_income) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.submit t'Create' %>
  </p>
<% end %>

<%= link_to t('Back'), fixed_incomes_path %>